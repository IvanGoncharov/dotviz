  "keywords": [
    "graphviz",
    "wasm",
    "WebAssembly"
  ],
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IvanGoncharov/dotviz.git"
  },
  "license": "MIT",
  "type": "module",
  "main": "./dist/viz.js",
  "exports": {
    "types": "./types/index.d.ts",
    "require": "./dist/viz.cjs",
    "default": "./dist/viz.js"
  },
  "files": [
    "dist",
    "lib",
    "src",
    "types"
  ],
  "devDependencies": {
    "@babel/core": "7.27.7",
    "@babel/preset-env": "7.27.2",
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-terser": "0.4.4",
    "jsdom": "26.1.0",
    "mocha": "11.7.1",
    "rollup": "4.44.1",
    "typedoc": "0.28.6",
    "typedoc-plugin-mdn-links": "5.0.2"
  },
  "scripts": {
    "test": "mocha",
    "docs": "typedoc types/index.d.ts"
  }
}
